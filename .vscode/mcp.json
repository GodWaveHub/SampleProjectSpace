// settings.jsonに各方式は旧バージョン。最新vscodeはmcp.jsonに統一
{
	"servers": {
        "postgres": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-postgres",
                "postgresql://postgres:postgres@localhost:5432/postgres"
            ]
        },
        "playwright": {
            "command": "npx",
            "args": [
                "-y",
                "@playwright/mcp"
            ]
        }
	},
	"inputs": []
}